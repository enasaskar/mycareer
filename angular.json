{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "my-career": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/my-career",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/vendor/bootstrap/bootstrap.min.css",
              "node_modules/bootstrap3/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "src/assets/vendor/owlcarousel/owl.carousel.min.css",
              "src/assets/vendor/owlcarousel/owl.theme.default.min.css",
              "src/assets/vendor/magnific-popup/magnific-popup.css",
              "src/assets/dist/bootstrap-star-rating/css/star-rating.min.css",
              "src/assets/css/review.css",              
              "src/assets/dist/bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css",
              "src/assets/css/theme.css",
              "src/assets/css/theme-elements.css",
              "src/assets/css/theme-blog.css",
              "src/assets/css/theme-shop.css",
              "src/assets/css/theme-animate.css",
              "src/assets/css/skins/default.css",
              "src/assets/css/custom.css",
              "src/assets/css/custommodal.css",
              "src/assets/css/pathStyle.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/vendor/modernizr/modernizr.js",                        
              "src/assets/vendor/jquery/jquery.js",
              "src/assets/vendor/jquery.appear/jquery.appear.js",
              "src/assets/vendor/jquery.easing/jquery.easing.js",
              "src/assets/vendor/jquery-cookie/jquery-cookie.js",
              "src/assets/vendor/bootstrap/bootstrap.min.js",   
              "src/assets/vendor/common/common.js",                         
              "src/assets/dist/moment.js/min/moment.min.js",
              "src/assets/vendor/jquery.validation/jquery.validation.js",
              "src/assets/vendor/jquery.stellar/jquery.stellar.js",
              "src/assets/vendor/jquery.easy-pie-chart/jquery.easy-pie-chart.js",
              "src/assets/vendor/jquery.gmap/jquery.gmap.js",
              "src/assets/vendor/isotope/jquery.isotope.js",
              "src/assets/vendor/owlcarousel/owl.carousel.js",
              "src/assets/vendor/jflickrfeed/jflickrfeed.js",
              "src/assets/vendor/magnific-popup/jquery.magnific-popup.js",      
              "src/assets/vendor/vide/vide.js",                                  
              "src/assets/js/theme.js",
              "src/assets/vendor/rs-plugin/js/jquery.themepunch.tools.min.js",
              "src/assets/vendor/rs-plugin/js/jquery.themepunch.revolution.min.js",
              "src/assets/vendor/circle-flip-slideshow/js/jquery.flipshow.js",
              "src/assets/js/views/view.home.js",              
              "src/assets/js/custom.js",
              "src/assets/js/theme.init.js",
              "src/assets/dist/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js",              
              "src/assets/js/datepicker.js",
              "src/assets/dist/bootstrap-star-rating/js/star-rating.min.js",
              "src/assets/js/review.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "my-career:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "my-career:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "my-career:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "my-career-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "my-career:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "my-career"
}